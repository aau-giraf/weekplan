{
  "cli": {
    "version": ">= 13.2.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "distribution": "store",
      "environment": "production",
      "credentialsSource": "remote",
      "env": {
        "EXPO_PUBLIC_API_URL": "https://weekpl.giraf.cs.aau.dk",
        "EXPO_PUBLIC_API_PORT": "443"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
